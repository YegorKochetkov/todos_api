{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": "xo",
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"indent": ["error", "tab"],
		"linebreak-style": ["error", "unix"],
		"quotes": ["error", "double"],
		"semi": ["error", "always"],
		"capitalized-comments": ["off"],
		"arrow-parens": "off",
		"object-curly-spacing": "off"
	}
}
