	},
	"type": "module",
	"dependencies": {
		"axios": "^1.3.5",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"lodash": "^4.17.21",
		"minimist": "^1.2.8",
		"pg": "^8.10.0",
		"pg-hstore": "^2.3.4",
		"sequelize": "^6.31.1",
		"sequelize-cli": "^6.6.0"
	},
	"devDependencies": {
		"@types/express": "^4.17.17",
		"@types/node": "^18.15.12",
		"eslint": "^8.37.0",
		"eslint-config-xo": "^0.43.1",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.0",
		"nodemon": "^2.0.22",
		"prettier": "2.8.7"
	},
	"lint-staged": {
		"*.js": "npm run format-lint"
